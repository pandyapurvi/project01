<div class="car-model">
<h1>Models of the cars</h1>

<% @models.each do |model| %>
<div class="brand-logo">
<% if model.image %>
<%= image_tag model.image, :class => 'model-thumb' %>
<% end %>
</div>
<ul>
  <div class="model-name" >
  <li> <%= link_to model.name, model_path(model.id), :class => "model" %></li>
</ul>
</div>
<div class="brand-temp"></div>
<% end %>
</div>
